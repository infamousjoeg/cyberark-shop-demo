- !policy
  id: {{ conjur_authn_service_id }}
  body:
    - !webservice

    - !variable token-app-property
    - !variable identity-path
    - !variable issuer
    - !variable jwks-uri
    - !variable ca-cert

    - !group apps

    - !permit
      role: !group apps
      privileges: [ read, authenticate ]
      resource: !webservice